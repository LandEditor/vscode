{
	"name": "debug-server-ready",
	"displayName": "%displayName%",
	"description": "%description%",
	"main": "./out/extension",
	"scripts": {
		"compile": "gulp compile-extension:debug-server-ready"
	},
	"contributes": {
		"debuggers": [
			{
				"configurationAttributes": {
					"launch": {
						"properties": {
							"serverReadyAction": {
								"oneOf": [
									{
										"additionalProperties": false,
										"default": {
											"action": "openExternally",
											"killOnServerStop": false
										},
										"markdownDescription": "%debug.server.ready.serverReadyAction.description%",
										"properties": {
											"action": {
												"default": "openExternally",
												"enum": [
													"openExternally"
												],
												"enumDescriptions": [
													"%debug.server.ready.action.openExternally.description%"
												],
												"markdownDescription": "%debug.server.ready.action.description%",
												"type": "string"
											},
											"killOnServerStop": {
												"default": false,
												"markdownDescription": "%debug.server.ready.killOnServerStop.description%",
												"type": "boolean"
											},
											"pattern": {
												"default": "listening on port ([0-9]+)",
												"markdownDescription": "%debug.server.ready.pattern.description%",
												"type": "string"
											},
											"uriFormat": {
												"default": "http://localhost:%s",
												"markdownDescription": "%debug.server.ready.uriFormat.description%",
												"type": "string"
											}
										},
										"type": "object"
									},
									{
										"additionalProperties": false,
										"default": {
											"action": "debugWithEdge",
											"killOnServerStop": false,
											"pattern": "listening on port ([0-9]+)",
											"uriFormat": "http://localhost:%s",
											"webRoot": "${workspaceFolder}"
										},
										"markdownDescription": "%debug.server.ready.serverReadyAction.description%",
										"properties": {
											"action": {
												"default": "debugWithEdge",
												"enum": [
													"debugWithChrome",
													"debugWithEdge"
												],
												"enumDescriptions": [
													"%debug.server.ready.action.debugWithChrome.description%"
												],
												"markdownDescription": "%debug.server.ready.action.description%",
												"type": "string"
											},
											"killOnServerStop": {
												"default": false,
												"markdownDescription": "%debug.server.ready.killOnServerStop.description%",
												"type": "boolean"
											},
											"pattern": {
												"default": "listening on port ([0-9]+)",
												"markdownDescription": "%debug.server.ready.pattern.description%",
												"type": "string"
											},
											"uriFormat": {
												"default": "http://localhost:%s",
												"markdownDescription": "%debug.server.ready.uriFormat.description%",
												"type": "string"
											},
											"webRoot": {
												"default": "${workspaceFolder}",
												"markdownDescription": "%debug.server.ready.webRoot.description%",
												"type": "string"
											}
										},
										"type": "object"
									},
									{
										"additionalProperties": false,
										"default": {
											"action": "startDebugging",
											"killOnServerStop": false,
											"name": "<launch browser config name>"
										},
										"markdownDescription": "%debug.server.ready.serverReadyAction.description%",
										"properties": {
											"action": {
												"default": "startDebugging",
												"enum": [
													"startDebugging"
												],
												"enumDescriptions": [
													"%debug.server.ready.action.startDebugging.description%"
												],
												"markdownDescription": "%debug.server.ready.action.description%",
												"type": "string"
											},
											"killOnServerStop": {
												"default": false,
												"markdownDescription": "%debug.server.ready.killOnServerStop.description%",
												"type": "boolean"
											},
											"name": {
												"default": "Launch Browser",
												"markdownDescription": "%debug.server.ready.debugConfigName.description%",
												"type": "string"
											},
											"pattern": {
												"default": "listening on port ([0-9]+)",
												"markdownDescription": "%debug.server.ready.pattern.description%",
												"type": "string"
											}
										},
										"required": [
											"name"
										],
										"type": "object"
									},
									{
										"additionalProperties": false,
										"default": {
											"action": "startDebugging",
											"config": {
												"request": "launch",
												"type": "node"
											},
											"killOnServerStop": false
										},
										"markdownDescription": "%debug.server.ready.serverReadyAction.description%",
										"properties": {
											"action": {
												"default": "startDebugging",
												"enum": [
													"startDebugging"
												],
												"enumDescriptions": [
													"%debug.server.ready.action.startDebugging.description%"
												],
												"markdownDescription": "%debug.server.ready.action.description%",
												"type": "string"
											},
											"config": {
												"default": {},
												"markdownDescription": "%debug.server.ready.debugConfig.description%",
												"type": "object"
											},
											"killOnServerStop": {
												"default": false,
												"markdownDescription": "%debug.server.ready.killOnServerStop.description%",
												"type": "boolean"
											},
											"pattern": {
												"default": "listening on port ([0-9]+)",
												"markdownDescription": "%debug.server.ready.pattern.description%",
												"type": "string"
											}
										},
										"required": [
											"config"
										],
										"type": "object"
									}
								]
							}
						}
					}
				},
				"type": "*"
			}
		]
	},
	"activationEvents": [
		"onDebugResolve"
	],
	"devDependencies": {
		"@types/node": "22.9.1"
	},
	"icon": "media/icon.png",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": false
	},
	"enabledApiProposals": [
		"terminalDataWriteEvent"
	]
}
