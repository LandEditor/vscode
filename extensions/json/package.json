{
	"contributes": {
		"grammars": [
			{
				"language": "json",
				"path": "./syntaxes/JSON.tmLanguage.json",
				"scopeName": "source.json"
			},
			{
				"language": "jsonc",
				"path": "./syntaxes/JSONC.tmLanguage.json",
				"scopeName": "source.json.comments"
			},
			{
				"language": "jsonl",
				"path": "./syntaxes/JSONL.tmLanguage.json",
				"scopeName": "source.json.lines"
			},
			{
				"language": "snippets",
				"path": "./syntaxes/snippets.tmLanguage.json",
				"scopeName": "source.json.comments.snippets"
			}
		],
		"languages": [
			{
				"aliases": [
					"JSON",
					"json"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".json",
					".bowerrc",
					".jscsrc",
					".webmanifest",
					".js.map",
					".css.map",
					".ts.map",
					".har",
					".jslintrc",
					".jsonld",
					".geojson",
					".ipynb",
					".vuerc"
				],
				"filenames": [
					"composer.lock",
					".watchmanconfig"
				],
				"id": "json",
				"mimetypes": [
					"application/json",
					"application/manifest+json"
				]
			},
			{
				"aliases": [
					"JSON with Comments"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".jsonc",
					".eslintrc",
					".eslintrc.json",
					".jsfmtrc",
					".jshintrc",
					".swcrc",
					".hintrc",
					".babelrc"
				],
				"filenames": [
					"babel.config.json",
					".babelrc.json",
					".ember-cli",
					"typedoc.json"
				],
				"id": "jsonc"
			},
			{
				"aliases": [
					"JSON Lines"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".jsonl"
				],
				"filenames": [],
				"id": "jsonl"
			},
			{
				"aliases": [
					"Code Snippets"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".code-snippets"
				],
				"filenamePatterns": [
					"**/User/snippets/*.json",
					"**/User/profiles/*/snippets/*.json"
				],
				"id": "snippets"
			}
		]
	},
	"description": "%description%",
	"displayName": "%displayName%",
	"name": "json",
	"scripts": {
		"update-grammar": "node ./build/update-grammars.js"
	}
}
