{
	"name": "vb",
	"displayName": "%displayName%",
	"version": "1.0.0",
	"description": "%description%",
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode.git"
	},
	"license": "MIT",
	"publisher": "vscode",
	"scripts": {
		"update-grammar": "node ../node_modules/vscode-grammar-updater/bin textmate/asp.vb.net.tmbundle Syntaxes/ASP%20VB.net.plist ./syntaxes/asp-vb-net.tmLanguage.json"
	},
	"contributes": {
		"grammars": [
			{
				"language": "vb",
				"path": "./syntaxes/asp-vb-net.tmLanguage.json",
				"scopeName": "source.asp.vb.net"
			}
		],
		"languages": [
			{
				"aliases": [
					"Visual Basic",
					"vb"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".vb",
					".brs",
					".vbs",
					".bas",
					".vba"
				],
				"id": "vb"
			}
		],
		"snippets": [
			{
				"language": "vb",
				"path": "./snippets/vb.code-snippets"
			}
		]
	},
	"dependencies": {},
	"devDependencies": {},
	"engines": {
		"vscode": "*"
	}
}
