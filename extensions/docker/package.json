{
	"name": "docker",
	"displayName": "%displayName%",
	"version": "1.0.0",
	"description": "%description%",
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode.git"
	},
	"license": "MIT",
	"publisher": "vscode",
	"scripts": {
		"update-grammar": "node ../node_modules/vscode-grammar-updater/bin moby/moby contrib/syntax/textmate/Docker.tmbundle/Syntaxes/Dockerfile.tmLanguage ./syntaxes/docker.tmLanguage.json"
	},
	"contributes": {
		"configurationDefaults": {
			"[dockerfile]": {
				"editor.quickSuggestions": {
					"strings": true
				}
			}
		},
		"grammars": [
			{
				"language": "dockerfile",
				"path": "./syntaxes/docker.tmLanguage.json",
				"scopeName": "source.dockerfile"
			}
		],
		"languages": [
			{
				"aliases": [
					"Docker",
					"Dockerfile",
					"Containerfile"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".dockerfile",
					".containerfile"
				],
				"filenamePatterns": [
					"Dockerfile.*",
					"Containerfile.*"
				],
				"filenames": [
					"Dockerfile",
					"Containerfile"
				],
				"id": "dockerfile"
			}
		]
	},
	"dependencies": {},
	"devDependencies": {},
	"engines": {
		"vscode": "*"
	}
}
