{
	"name": "github-authentication",
	"displayName": "%displayName%",
	"description": "%description%",
	"main": "./out/extension.js",
	"browser": "./dist/browser/extension.js",
	"scripts": {
		"compile": "gulp compile-extension:github-authentication",
		"compile-web": "npx webpack-cli --config extension-browser.webpack.config --mode none",
		"watch-web": "npx webpack-cli --config extension-browser.webpack.config --mode none --watch --info-verbosity verbose"
	},
	"contributes": {
		"authentication": [
			{
				"id": "github",
				"label": "GitHub"
			},
			{
				"id": "github-enterprise",
				"label": "GitHub Enterprise Server"
			}
		],
		"configuration": [
			{
				"properties": {
					"github-enterprise.uri": {
						"description": "GitHub Enterprise Server URI",
						"type": "string"
					}
				},
				"title": "GitHub Enterprise Server Authentication Provider"
			},
			{
				"properties": {
					"github.experimental.multipleAccounts": {
						"description": "Experimental support for multiple GitHub accounts",
						"type": "boolean"
					}
				},
				"title": "GitHub Authentication"
			}
		]
	},
	"activationEvents": [],
	"dependencies": {
		"node-fetch": "3.3.2"
	},
	"devDependencies": {
		"@types/node": "22.x",
		"@types/node-fetch": "2.6.11"
	},
	"icon": "images/icon.png",
	"aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
	"api": "none",
	"capabilities": {
		"untrustedWorkspaces": {
			"restrictedConfigurations": [
				"github-enterprise.uri"
			],
			"supported": "limited"
		},
		"virtualWorkspaces": true
	},
	"extensionKind": [
		"ui",
		"workspace"
	]
}
