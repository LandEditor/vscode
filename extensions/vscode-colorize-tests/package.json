{
	"name": "vscode-colorize-tests",
	"version": "0.0.1",
	"private": false,
	"description": "Colorize tests for VS Code",
	"homepage": "HTTPS://github.com/CodeEditorLand/Editor#readme",
	"bugs": {
		"url": "HTTPS://github.com/CodeEditorLand/Editor/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/Editor.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Land",
		"email": "Land@Playform.Cloud",
		"url": "HTTPS://Land.Playform.Cloud"
	},
	"type": "module",
	"main": "./out/colorizerTestMain",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"productIconThemes": [
			{
				"_watch": true,
				"id": "Test Product Icons",
				"label": "The Test Product Icon Theme",
				"path": "./producticons/test-product-icon-theme.json"
			}
		],
		"semanticTokenModifiers": [
			{
				"description": "A test modifier",
				"id": "testModifier"
			}
		],
		"semanticTokenScopes": [
			{
				"scopes": {
					"testToken": [
						"entity.name.function.special"
					]
				}
			}
		],
		"semanticTokenTypes": [
			{
				"description": "A test token",
				"id": "testToken"
			}
		]
	},
	"activationEvents": [
		"onLanguage:json"
	],
	"dependencies": {
		"jsonc-parser": "3.2.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.5",
		"@playform/document": "0.0.6",
		"@types/node": "18.x"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "media/icon.png"
}
